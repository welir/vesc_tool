<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Dark">
    <style name="Text" foreground="#B4B4B4" background="#272727"/>
    <style name="Link" foreground="#8888ff"/>
    <style name="Selection" foreground="#eff0f1" background="#0d3e59"/>
    <style name="LineNumber" foreground="#6272a4"/>
    <style name="SearchResult" background="#3f82a6"/>
    <style name="SearchScope" background="#2d5c76"/>
