    <style name="Global" foreground="#ce5c00"/>
    <style name="Field" foreground="#800000"/>
    <style name="Static" foreground="#800080"/>
    <style name="VirtualMethod" foreground="#00677c" background="#272727" italic="true"/>
    <style name="Function" foreground="#a0c7fc" background="#272727"/>
    <style name="Keyword" foreground="#ffffa0"/>
    <style name="PrimitiveType" foreground="#ffffa0"/>
    <style name="Operator"/>
    <style name="Overloaded Operator"/>
    <style name="Preprocessor" foreground="#a0a0ff"/>
    <style name="Label" foreground="#ffa0a0"/>
    <style name="Comment" foreground="#a0cca0"/>
    <style name="Doxygen.Comment" foreground="#a0a0ff"/>
    <style name="Doxygen.Tag" foreground="#a0a0ff"/>
    <style name="VisualWhitespace" foreground="#303030"/>
    <style name="QmlLocalId" foreground="#B4B4B4" background="#272727" italic="true"/>
    <style name="QmlExternalId" foreground="#a0a0ff" background="#272727" italic="true"/>
    <style name="QmlTypeId" foreground="#ffa0ff"/>
    <style name="QmlRootObjectProperty" foreground="#B4B4B4" background="#272727" italic="true"/>
    <style name="QmlScopeObjectProperty" foreground="#B4B4B4" background="#272727" italic="true"/>
    <style name="QmlExternalObjectProperty" foreground="#a0a0ff" background="#272727" italic="true"/>
    <style name="JsScopeVar" foreground="#2985c7" background="#272727" italic="true"/>
    <style name="JsImportVar" foreground="#a0b5ff" background="#272727" italic="true"/>
    <style name="JsGlobalVar" foreground="#a0b5ff" background="#272727" italic="true"/>
    <style name="QmlStateName" foreground="#B4B4B4" background="#272727" italic="true"/>
    <style name="Binding" foreground="#800000"/>
